{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "phases": {
        "setup": {
          "nixPkgs": ["python311", "gcc", "gomp", "glibc"]
        },
        "install": {
          "cmds": [
            "pip install -r requirements.txt"
          ]
        }
      }
    }
  },
  "deploy": {
    "startCommand": "gunicorn VisAutoML.wsgi"
  }
}
